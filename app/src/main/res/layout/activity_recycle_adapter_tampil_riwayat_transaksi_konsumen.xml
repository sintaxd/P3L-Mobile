<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
android:layout_width="match_parent"
android:layout_height="wrap_content"
android:layout_marginBottom="20dp"
android:background="@color/menusKonsumen2"
android:orientation="vertical"
android:id="@+id/row_container">

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        android:id="@+id/tanggal_transaksi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tanggal Transaksi" />

    <TextView
        android:id="@+id/nama_jasa_service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nama Jasa Service" />

    <TextView
        android:id="@+id/harga_jasa_service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Harga Jasa Service" />

    <TextView
        android:id="@+id/tipe_motor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tipe Motor" />

    <TextView
        android:id="@+id/status_service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Status Service" />

</LinearLayout>


</RelativeLayout>