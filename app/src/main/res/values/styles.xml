<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="OwnerTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/menus</item>
        <item name="colorPrimaryDark">@color/menus</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="CSViewTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/menusCS</item>
        <item name="colorPrimaryDark">@color/menusCS</item>
        <item name="colorAccent">@color/menusCS2</item>
    </style>

    <style name="CSTheme2" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/menusCS</item>
        <item name="colorPrimaryDark">@color/menusCS</item>
        <item name="colorAccent">@color/menusCS2</item>
    </style>
    <style name="KonsumenViewTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorKonsumen</item>
        <item name="colorPrimaryDark">@color/menusKonsumen</item>
        <item name="colorAccent">@color/menusKonsumen2</item>
</style>
    <style name="KonsumenTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorKonsumen</item>
        <item name="colorPrimaryDark">@color/menusKonsumen</item>
        <item name="colorAccent">@color/menusKonsumen2</item>
    </style>
</resources>
